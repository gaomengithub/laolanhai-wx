.filter {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  padding: 0 22rpx;
  height: 84rpx;
  background-color: #ffffff;
}

.filter .cell {
  height: 72rpx !important;
  width: 260rpx !important;
  border-radius: 44rpx !important;
  color: #000;
  border: none !important;
  background-color: #F2F4F3 !important;
  display: flex;
  align-items: center;
  position: relative;
}

.small {
  height: 72rpx !important;
  border: none !important;
  width: 116rpx !important;
  border-radius: 44rpx !important;
}

.filter .cell .child {
  width: 190rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 32rpx;
  letter-spacing: 2rpx;
}


.filter .cell .icon {
  position: absolute;
  top: 25.5rpx;
  right: 15rpx;
  width: 25rpx;
  height: 25rpx;
}

.filter .search {
  height: 72rpx !important;
  width: 116rpx !important;
  border-radius: 40rpx;
  display: flex;
  flex-direction: column;
}