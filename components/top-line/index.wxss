.container {
  position: relative;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 100%;
}

.main {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  height: 90rpx;
  width: 90%;
}

.name {
  position: relative;
  width: fit-content;
  font-size: 34rpx;
  font-weight: 600;
  z-index: 1;
}

.name::after {
  content: "";
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 33%;
  background-color: #FB8808;
  z-index: -1;
}

.more {
  width: fit-content;
  color: #969696;
  position: relative;
}


.hover {
  background-color: #ffffff;
}