.container {
  position: fixed;
  bottom: 0;
  height: 166rpx;
  padding: 0 44rpx 0 44rpx;
  box-shadow: 0px -2px 10px rgba(0, 0, 0, 0.1);
  z-index: 10;
}

.main {
  width: 662rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-direction: row;
  padding-top: 16rpx;
}

.btn {
  width: 550rpx;
  height: 100rpx;
  font-weight: 600;
  letter-spacing: 2rpx;
  font-size: 34rpx;
}

