.container {
  position: relative;
}

.swiper-img {
  width: 100%;
}

.sub-container {
  position: relative;
  margin-top: -200rpx;
}

.price-item {
  position: absolute;
  z-index: 1;
  top: -100rpx;
  right: 12rpx;
  height: 185rpx;
  width: 185rpx;
  background-image: url('https://openstore.obabyball.com/ui_v1/icon/octagonal-star.svg');
  background-size: contain;
  display: flex;
  justify-content: center;
  align-items: center;
}

.mask {
  position: relative;
  height: 200rpx;
  background: linear-gradient(to top, rgba(99, 99, 99, 1), rgba(99, 99, 99, 0));
  /* box-shadow: 0 0 20px rgba(99, 99, 99, 1); */
}

.mask .type {
  width: 180rpx;
  margin-left: 44rpx;
}

.mask .name {
  display: -webkit-box;
  word-break: break-all;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-left: 44rpx;
  color: #ffffff;
  font-size: 48rpx;
  font-weight: 600;
  width: 634rpx;
  letter-spacing: 2rpx;
}

.cost-tip {
  font-size: 42rpx;
  font-weight: 600;
  color: #FFFFFF;
}

/* .star-icon {
  height: 200rpx;
  width: 200rpx;
} */

.mid {
  position: relative;
  margin-top: -2rpx;
  display: flex;
  flex-direction: column;
  height: fit-content;
  width: 100%;
  justify-content: space-between;
  padding-top: 44rpx;
  padding-bottom: 44rpx;
  background-color: rgba(99, 99, 99, 1);
  /* box-shadow: 0 0 20px rgba(99, 99, 99, 1); */
}

.mid .date-address {
  background-color: rgba(0, 0, 0, 0.4);
  border-radius: 44rpx;
  color: #fff;
  font-weight: 600;
  font-size: 36rpx;
  padding: 22rpx 0 22rpx 22rpx;
  margin: 0 40rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: center;
}

.mid .date-address .cell {
  display: flex;
  flex-direction: row;
  padding: 5px;
  align-items: center;
}

.mid .date-address .cell .child {
  height: 32rpx;
  width: 32rpx;
  margin-right: 20rpx;
}


.mid .part {
  background-color: rgba(0, 0, 0, 0.4);
  border-radius: 44rpx;
  padding: 32rpx;
  margin: 24rpx 40rpx;
}

.mid .part .title {
  font-size: 30rpx;
  color: #FFFFFF;
  opacity: 0.6;
}

.part .avatars {
  width: 606rpx;
  height: fit-content;
  padding: 22rpx 0 0 0;
  overflow: hidden;
}

.part .avatar {
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  border: 4rpx, solid, #fff;
  margin: 6rpx;
}

.desc {
  position: relative;
  background-color: #ffffff;
  border-radius: 22px;
  top: -22px;
  padding: 22px;
}

.desc .title {
  font-size: 36rpx;
  font-weight: 700;
  width: fit-content;
  background: -webkit-linear-gradient(top,
      rgba(230, 115, 9, 0) 0%,
      rgba(230, 115, 9, 0) 50%,
      rgb(230, 115, 9, 1) 100%);
  /* 渐变效果，从透明到黑色，可根据需要调整 */
}

.desc .content {
  font-size: 36rpx;
}

.desc .age {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 36rpx;
  font-weight: 600;
  letter-spacing: 2rpx;
}

.active {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 166rpx;
  background: white;
  display: flex;
  justify-content: center;
  align-items: flex-start;
  /* padding-bottom: env(safe-area-inset-bottom); */
}

.btn-custom-class {
  margin-top: 16rpx;
  color: #FB8808 !important;
  font-size: 36rpx;
  font-weight: 600;
  width: 650rpx !important;
  background-color: #000000 !important;
  border-radius: 60rpx !important;
}



.tip-icon {
  position: relative;
  padding: 30rpx;
  left: 50%;
  transform: translateX(-50%);
}

.confirm-btn {
  --font-weight-bold: 600 !important;
  color: #000000 !important;
  --button-danger-background-color: #FB8808 !important;
}

.dialog .msg {
  text-align: center;
  font-size: 36rpx;
  font-weight: 600;
  padding-top: 22rpx;
  padding-bottom: 22rpx;
}

.edit {
  margin-top: 6rpx;
  height: 65rpx;
  width: 65rpx;
}

.cell-items {
  width: 600rpx !important;
  padding: 22rpx 0;
  /* letter-spacing: 4rpx; */
}

.cell-title {
  font-size: 32rpx !important;
  font-weight: 600;
}

.btn {
  color: #fff !important;
  font-size: 36rpx !important;
  font-weight: 600;
  letter-spacing: 8rpx;
  height: 100rpx !important;
  border: none !important;
  background-color: #FB8808 !important;
}

.radio-group {
  margin: 48rpx 12rpx;
}

.code {
  width: 654rpx;
  height: 654rpx;
  padding: 24rpx;
}