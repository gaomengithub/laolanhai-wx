page {
  background-color: #ebebeb;
}

.avatar {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin: 12rpx;
}

.name {
  font-size: 36rpx;
}

.dialog-container {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.grid {
  display: flex;
  flex-wrap: wrap;
  margin: 24rpx 0;
}

.cell {
  display: flex;
  align-items: center;
  border-radius: 22rpx;
  justify-content: center;
  width: 279rpx;
  height: 95rpx;
  margin: 12rpx 24rpx;
  background-color: #fb8808;
  color: #ffffff;
}

.miss {
  background-color: #989898;
}