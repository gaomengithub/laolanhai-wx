.container {
  background-color: #E2E2E2;
  min-height: 100vh;
  width: 100%;
}

.create {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  margin: 22rpx 48rpx 22rpx 48rpx;
  background-color: #FAFAFA;
  border-radius: 48rpx;
  height: 88rpx;
}

.icon {
  width: 66rpx;
  height: 66rpx;
}

.btn {
  width: 165rpx !important;
  height: 66rpx !important;
}

.btn .text {
  color: #FB8808;
}

.tab {
  font-size: 32rpx !important;
}