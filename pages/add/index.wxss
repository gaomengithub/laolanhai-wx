.container {
  display: flex;
  flex-direction: column;
  margin: 22px;
}

.active {
  width: 100%;
  background-color: #ffffff;
  position: fixed;
  bottom: 0;
  height: 166rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  z-index: 1;
  box-shadow: 0px -2px 10px rgba(0, 0, 0, 0.1);
  /* 添加此行来产生阴影 */
}

.btn-custom-class {
  margin-top: 16rpx;
  width: 650rpx;
  background-color: #FB8808 !important;
  font-weight: 600;
}

.upload-bg-img {
  width: 170rpx;
  height: 174rpx;
}

.van-cell {
