<view class="item" wx:for="{{players}}" wx:key="index">
  <image class="avatar" src="{{item.avatar}}" mode="aspectFill"></image>
  <view class="info">
    <view class="name">
      {{ item.nickName }}
    </view>
    <!-- <view class="position">
    {{position}}
    </view> -->
  </view>
  <view wx:if="{{ isLeader }}" class="type">
    队长
  </view>
</view>