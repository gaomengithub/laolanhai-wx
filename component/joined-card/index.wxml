<view class="card">
  <top-line name="我已参加的"></top-line>
  <view wx:for="{{ items }}" wx:key="index" class="item">
    <image class="type-icon" src="{{ item.match_type == 3 ? unOfficialIcon : officialIcon}}" mode="aspectFill" />
    <view class="content">
      <view class="name">
        {{item.name}}
      </view>
      <view class="type">
        <wxs src="./index.wxs" module="tools" />
        <block>
          {{ tools.getTypeName(item.match_type) }}
        </block>
      </view>
    </view>
    <view class="status">
      已结束
    </view>
    <navigator url="/pages/sub/create-modify-match/index?class=modify&type={{item.match_type}}&matchID={{ item.id }}">
      <image class="link" src="{{linkIcon}}" mode="aspectFill" />
    </navigator>
  </view>
</view>